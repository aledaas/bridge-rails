{
  "name": "aledaas/bridge-rails",
  "description": "Bridge.xyz rails driver for Laravel (API client + webhook verification).",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Aledaas\\BridgeRails\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aledaas\\BridgeRails\\BridgeRailsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
